<header class=" group">
	<div class="inner">
		<div class="logo">
			<a href="/events">Play My Song</a>
		</div>
		<ul>
			<% if user_signed_in? %>
			<li>
				<%= link_to "Events", events_path %>
			</li>
			<li>
				<%= link_to "Songs", songs_path %>
			</li>
			<li>
				<%= link_to "Log Out", destroy_user_session_path, method: :delete %>
			</li>
			<% else %>
			<li>
				<%= link_to "Request Songs", welcome_path %>
			</li>
			<li>
				<%= link_to "Log In", new_user_session_path %>
			</li>
			<li>
				<a href="/users/sign_up">Sign Up</a>
			</li>
			<% end %>
		</ul>
	</div>
</header>
